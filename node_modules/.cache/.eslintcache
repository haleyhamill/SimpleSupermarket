unt":0,"fatalErrorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},{"filePath":"63","messages":"64","errorCount":0,"fatalErrorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},"/Users/haleyhamill/Desktop/github/SimpleSupermarket/src/index.js",["65"],"import \"./index.css\";\nimport React, { useState, useEffect } from \"react\";\nimport {render} from \"react-dom\";\nimport { BrowserRouter, Switch, Route } from \"react-router-dom\";\nimport Navbar from \"./Navbar.js\";\nimport Home from \"./Home.js\";\nimport About from \"./About.js\";\nimport Products from \"./Products.js\";\nimport ProductDetails from \"./ProductDetails.js\";\nimport Cart from \"./Cart.js\";\n\nfunction App() {\n  const [cart, setCart] = useState([]);\n\n  function handleProductDelete(id) {\n    const updatedCart = cart.filter((product) => product.id !== id);\n    setCart(updatedCart);\n  }\n\n  function handleProductAdd(newProduct) {\n    // check if item exists\n    const existingProduct = cart.find(\n      (product) => product.id === newProduct.id\n    );\n    if (existingProduct) {\n      // increase quantity\n      const updatedCart = cart.map((product) => {\n        if (product.id === newProduct.id) {\n          return {\n            ...product,\n            quantity: product.quantity + 1,\n          };\n        }\n        return product;\n      });\n      setCart(updatedCart);\n    } else {\n      // product is new to the cart\n      setCart([\n        ...cart,\n        {\n          ...newProduct,\n          quantity: 1,\n        },\n      ]);\n    }\n  }\n\n  return (\n    <BrowserRouter>\n      <Navbar cart={cart} />\n      <div className=\"container\">\n        <Switch>\n          <Route exact path=\"/\">\n            <Home />\n          </Route>\n          <Route exact path=\"/about\">\n            <About />\n          </Route>\n          <Route exact path=\"/products\">\n            <Products\n              cart={cart}\n              onProductAdd={handleProductAdd}\n              onProductDelete={handleProductDelete}\n            />\n          </Route>\n          <Route path=\"/products/:id\">\n            <ProductDetails onProductAdd={handleProductAdd} />\n          </Route>\n          <Route exact path=\"/cart\">\n            <Cart cart={cart} />\n          </Route>\n        </Switch>\n      </div>\n    </BrowserRouter>\n  );\n}\n\nrender(<App />, document.getElementById(\"root\"));","/Users/haleyhamill/Desktop/github/SimpleSupermarket/src/Home.js",[],"/Users/haleyhamill/Desktop/github/SimpleSupermarket/src/Navbar.js",[],"/Users/haleyhamill/Desktop/github/SimpleSupermarket/src/Products.js",["66"],"import './index.css';\nimport React, { useState, useEffect } from \"react\";\nimport Product from \"./Product.js\";\nimport useFetch from \"./useFetch.js\";\nimport Loader from \"./Loader.js\";\n\nexport default function Products(props) {\n  const [products, setProducts] = useState([]);\n  const { get, loading } = useFetch(\n    \"https://react-tutorial-demo.firebaseio.com/\"\n  );\n\n  useEffect(() => {\n    get(\"supermarket.json\")\n      .then((data) => {\n        setProducts(data);\n      })\n      .catch((error) => console.log(\"Could not load products\", error));\n  }, []);\n\n  return (\n    <div className=\"products-layout\">\n      <h1>Products</h1>\n      <p>Take a look at our products</p>\n      <div className=\"products-grid\">\n        {loading && <Loader />}\n        {products.map((product) => {\n          return (\n            <Product\n              cart={props.cart}\n              key={product.id}\n              details={product}\n              onProductAdd={props.onProductAdd}\n              onProductDelete={props.onProductDelete}\n            ></Product>\n          );\n        })}\n      </div>\n    </div>\n  );\n}","/Users/haleyhamill/Desktop/github/SimpleSupermarket/src/About.js",[],"/Users/haleyhamill/Desktop/github/SimpleSupermarket/src/ProductDetails.js",["67"],"import './index.css';\nimport React, { useState, useEffect } from \"react\";\nimport {NavLink, Switch, Route, useParams, useRouteMatch} from \"react-router-dom\";\nimport useFetch from \"./useFetch.js\";\nimport ProductDetailInfo from \"./ProductDetailInfo.js\";\nimport ProductDetailNutrition from \"./ProductDetailNutrition.js\";\nimport ProductDetailStorage from \"./ProductDetailStorage.js\";\n\nexport default function ProductDetails(props) {\n  const [product, setProduct] = useState({});\n  const { get } = useFetch(\"https://react-tutorial-demo.firebaseio.com/\");\n  const params = useParams();\n  const match = useRouteMatch();\n\n  useEffect(() => {\n    get(`productinfo/id${params.id}.json`)\n      .then((data) => {\n        setProduct(data);\n      })\n      .catch((error) => console.log(\"Could not load product details\", error));\n  }, []);\n\n  return (\n    <div className=\"product-details-layout\">\n      <div>\n        <h2>{product.name}</h2>\n        <img\n          src={product.image}\n          width=\"125\"\n          height=\"125\"\n          className=\"product-details-image\"\n          alt={product.name}\n        />\n      </div>\n      <div>\n        <div className=\"tabs\">\n          <ul>\n            <li>\n              <NavLink exact activeClassName=\"tab-active\" to={match.url}>\n                Details\n              </NavLink>\n            </li>\n            <li>\n              <NavLink\n                exact\n                activeClassName=\"tab-active\"\n                to={match.url + \"/nutrition\"}\n              >\n                Nutrition\n              </NavLink>\n            </li>\n            <li>\n              <NavLink\n                exact\n                activeClassName=\"tab-active\"\n                to={match.url + \"/storage\"}\n              >\n                Storage\n              </NavLink>\n            </li>\n          </ul>\n        </div>\n        <Switch>\n          <Route exact path={match.path}>\n            <ProductDetailInfo product={product} onProductAdd={props.onProductAdd} />\n          </Route>\n\n          <Route path={match.path + \"/nutrition\"}>\n            <ProductDetailNutrition nutrition={product.nutrition} />\n          </Route>\n\n          <Route path={match.path + \"/storage\"}>\n            <ProductDetailStorage storage={product.storage} />\n          </Route>\n        </Switch>\n      </div>\n    </div>\n  );\n}","/Users/haleyhamill/Desktop/github/SimpleSupermarket/src/Cart.js",[],"/Users/haleyhamill/Desktop/github/SimpleSupermarket/src/useFetch.js",[],"/Users/haleyhamill/Desktop/github/SimpleSupermarket/src/ProductDetailNutrition.js",[],"/Users/haleyhamill/Desktop/github/SimpleSupermarket/src/ProductDetailStorage.js",[],"/Users/haleyhamill/Desktop/github/SimpleSupermarket/src/Button.js",[],"/Users/haleyhamill/Desktop/github/SimpleSupermarket/src/Product.js",[],"/Users/haleyhamill/Desktop/github/SimpleSupermarket/src/Loader.js",[],"/Users/haleyhamill/Desktop/github/SimpleSupermarket/src/ProductDetailInfo.js",[],"/Users/haleyhamill/Desktop/github/SimpleSupermarket/src/Input.js",[],{"ruleId":"68","severity":1,"message":"69","line":2,"column":27,"nodeType":"70","messageId":"71","endLine":2,"endColumn":36},{"ruleId":"72","severity":1,"message":"73","line":19,"column":6,"nodeType":"74","endLine":19,"endColumn":8,"suggestions":"75"},{"ruleId":"72","severity":1,"message":"76","line":21,"column":6,"nodeType":"74","endLine":21,"endColumn":8,"suggestions":"77"},"no-unused-vars","'useEffect' is defined but never used.","Identifier","unusedVar","react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'get'. Either include it or remove the dependency array.","ArrayExpression",["78"],"React Hook useEffect has missing dependencies: 'get' and 'params.id'. Either include them or remove the dependency array.",["79"],{"desc":"80","fix":"81"},{"desc":"82","fix":"83"},"Update the dependencies array to be: [get]",{"range":"84","text":"85"},"Update the dependencies array 